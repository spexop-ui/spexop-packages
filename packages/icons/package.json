{
  "name": "@spexop/icons",
  "version": "0.1.0",
  "description": "Lightweight, framework-agnostic SVG icon library with zero dependencies",
  "author": "Spexop Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spexop-ui/design-system.git",
    "directory": "packages/icons"
  },
  "homepage": "https://www.spexop.com",
  "bugs": {
    "url": "https://github.com/spexop-ui/design-system/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./alert": {
      "types": "./dist/alert.d.ts",
      "import": "./dist/alert.js"
    },
    "./navigation": {
      "types": "./dist/navigation.d.ts",
      "import": "./dist/navigation.js"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "import": "./dist/actions.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.js"
    },
    "./files": {
      "types": "./dist/files.d.ts",
      "import": "./dist/files.js"
    },
    "./communication": {
      "types": "./dist/communication.d.ts",
      "import": "./dist/communication.js"
    },
    "./social": {
      "types": "./dist/social.d.ts",
      "import": "./dist/social.js"
    }
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "keywords": [
    "icons",
    "svg",
    "spexop",
    "design-system",
    "ui",
    "typescript",
    "react",
    "vue",
    "angular",
    "svelte",
    "framework-agnostic",
    "accessible",
    "tree-shakeable",
    "zero-dependencies"
  ],
  "devDependencies": {
    "@spexop/tsconfig": "workspace:*",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0"
  }
}
