{
  "name": "@spexop/icons",
  "version": "0.6.0",
  "description": "Professional icon library with 274 carefully curated icons for modern web applications. Includes filled variants and popular brand icons.",
  "author": "Spexop Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spexop-ui/spexop-packages.git",
    "directory": "packages/icons"
  },
  "homepage": "https://www.spexop.com",
  "bugs": {
    "url": "https://github.com/spexop-ui/spexop-packages/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "pnpm run clean && pnpm run build:icons && tsup",
    "build:icons": "tsx scripts/build-icons.ts",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "pnpm run build"
  },
  "files": ["dist/**/*", "README.md"],
  "keywords": [
    "react",
    "icons",
    "svg",
    "spexop",
    "design-system",
    "components",
    "typescript",
    "tree-shakeable",
    "accessible",
    "brand-icons",
    "social-media",
    "ui-icons"
  ],
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@spexop/tsconfig": "workspace:*",
    "@types/react": "^18.0.0",
    "react": "^18.3.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.6.0",
    "tsx": "^4.7.0",
    "svgo": "^3.2.0"
  }
}
